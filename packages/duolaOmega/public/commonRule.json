[{
  "id": 1000,
  "priority": 1,
  "condition": {
    "urlFilter": "*://*/*",
    "resourceTypes": [
      "script",
      "stylesheet",
      "image",
      "object",
      "other",
      "main_frame",
      "sub_frame",
      "websocket"
    ]
  },
  "action": {
    "type": "modifyHeaders",
    "responseHeaders": [
      {
        "header": "Access-Control-Allow-Origin",
        "operation": "set",
        "value": "*"
      },
      {
        "header": "Access-Control-Allow-Headers",
        "operation": "set",
        "value": "*"
      },
      {
        "header": "Access-Control-Allow-Methods",
        "operation": "set",
        "value": "*"
      },
      {
        "header": "Content-Security-Policy",
        "operation": "remove"
      },
      {
        "header": "X-Frame-Options",
        "operation": "remove"
      },
      {
        "header": "frame-ancestors",
        "operation": "remove"
      }
    ]
  }
}, {
  "id": 2000,
  "priority": 1,
  "condition": {
    "urlFilter":
    "*://*/statics/cdn/js/??react/react.production.min.js,react-dom/react-dom.production.min.js,react-router-dom/react-router-dom.min.js,moment/moment-with-locales.js,antd/antd-with-locales.min.js",
    "resourceTypes": ["script"]
  },
  "action": {
    "type": "redirect",
    "redirect": {
      "transform": {
        "path": "/statics/cdn/js/",
        "query":
        "??react/react.development.js,react-dom/react-dom.development.js,react-router-dom/react-router-dom.min.js,moment/moment-with-locales.js,antd/antd-with-locales.min.js"
      }
    }
  }
}, {
  "id": 3000,
  "priority": 1,
  "condition": {
    "urlFilter": "hot-update.json",
    "resourceTypes": ["xmlhttprequest"]
  },
  "action": {
    "type": "modifyHeaders",
    "responseHeaders": [
      {
        "header": "Access-Control-Allow-Origin",
        "operation": "set",
        "value": "*"
      },
      {
        "header": "Access-Control-Allow-Headers",
        "operation": "set",
        "value": "*"
      },
      {
        "header": "Access-Control-Allow-Methods",
        "operation": "set",
        "value": "*"
      }
    ]
  }
}]
