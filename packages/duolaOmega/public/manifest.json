{
  "manifest_version": 3,
  "name": "duolaOmega",
  "version": "1.0",
  "description": "SwitchyOmega similar",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "scripting",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon.png",
      "32": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "default_title": "duolaOmega panel",
    "default_popup": "index.html"
  }
}
